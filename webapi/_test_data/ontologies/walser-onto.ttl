## Class TEXTMANIFEST       
## 

:textmanifest rdf:type owl:Class ;
              rdfs:subClassOf knora-base:Resource   ,				
				 [
					rdf:type owl:Restriction;
					owl:onProperty :text-title;
					owl:cardinality "1"^^xsd:nonNegativeInteger  ;					
					salsah-gui:guiOrder "0"^^xsd:nonNegativeInteger] ,				
				 [
					rdf:type owl:Restriction;
					owl:onProperty :text-incipit;
					owl:cardinality "1"^^xsd:nonNegativeInteger  ;					
					salsah-gui:guiOrder "1"^^xsd:nonNegativeInteger] ,
				[
					rdf:type owl:Restriction;
					owl:onProperty :textmanifest-fulltext;
					owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
					salsah-gui:guiOrder "2"^^xsd:nonNegativeInteger ;]
					, 									 
				 [
					rdf:type owl:Restriction;
					owl:onProperty :text-kwa-volume;
					owl:maxCardinality "1"^^xsd:nonNegativeInteger  ;					
					salsah-gui:guiOrder "5"^^xsd:nonNegativeInteger] ,
				 [
					rdf:type owl:Restriction;
					owl:onProperty :text-kwa-page-no;
					owl:maxCardinality "1"^^xsd:nonNegativeInteger  ;					
					salsah-gui:guiOrder "6"^^xsd:nonNegativeInteger] ;
					
              rdfs:comment "A single manifestation of a text (on a textcarrier)" ;
              rdfs:label "Textmanifest"@de ,
              "text manifest"@en .      
              
              
