           knora-base:hasDefaultViewPermission knora-base:KnownUser ,
                                               knora-base:UnknownUser ,
                                               knora-base:ProjectMember ;

           knora-base:hasDefaultModifyPermission knora-base:Owner .

           
:on-surface rdf:type owl:ObjectProperty ;

           rdfs:subPropertyOf knora-base:hasLinkTo ;

           rdfs:label "Auf Textträger-Seite"@de ,                      
                      "On surface"@en ;
                      
           knora-base:subjectClassConstraint :column;
          
           knora-base:objectClassConstraint :surface ;                      

           salsah-gui:guiElement salsah-gui:Searchbox ;

           salsah-gui:guiAttribute "numprops=2" ;

           knora-base:hasDefaultDeletePermission knora-base:ProjectMember ,
                                                 knora-base:Owner .

:on-surfaceValue rdf:type owl:ObjectProperty ;

                rdfs:subPropertyOf knora-base:hasLinkToValue ;
              
                rdfs:label "In Textkolumne"@de ,                      
                      		"On surface column"@en ;
              
                knora-base:subjectClassConstraint :textmanifest ;
          		knora-base:objectClassConstraint knora-base:LinkValue .    
          		
:surface-notation-no  rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf  knora-base:hasValue ;
          rdfs:label "Seitenzahl"@de ,
                     "page no."@en ;

          rdfs:comment "The sequence number of the notation on the page" ;

          knora-base:subjectClassConstraint :column ;
          
          knora-base:objectClassConstraint knora-base:TextValue ;

          salsah-gui:guiElement salsah-gui:SimpleText ;

