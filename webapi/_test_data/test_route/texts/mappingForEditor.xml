<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="../../../src/main/resources/mappingXMLToStandoff.xsd">
    <mappingElement>
        <tag>
            <name>text</name>
            <class>noClass</class>
            <namespace>noNamespace</namespace>
            <separatesWords>false</separatesWords>
        </tag>
        <standoffClass>
            <classIri>http://www.knora.org/ontology/standoff#StandoffRootTag</classIri>
            <attributes>
                <attribute>
                    <attributeName>documentType</attributeName>
                    <namespace>noNamespace</namespace>
                    <propertyIri>http://www.knora.org/ontology/standoff#standoffRootTagHasDocumentType</propertyIri>
                </attribute>
            </attributes>
        </standoffClass>
    </mappingElement>

    <mappingElement>
        <tag>
            <name>p</name>
            <class>noClass</class>
            <namespace>noNamespace</namespace>
            <separatesWords>true</separatesWords>
        </tag>
        <standoffClass>
            <classIri>http://www.knora.org/ontology/standoff#StandoffParagraphTag</classIri>
        </standoffClass>
    </mappingElement>

    <mappingElement>
        <tag>
            <name>section</name>
            <class>noClass</class>
            <namespace>noNamespace</namespace>
            <separatesWords>false</separatesWords>
        </tag>
        <standoffClass>
            <classIri>http://www.knora.org/ontology/standoff#StandoffSectionTag</classIri>
        </standoffClass>
    </mappingElement>

    <mappingElement>
        <tag>
            <name>section</name>
            <class>footnotes</class>
            <namespace>noNamespace</namespace>
            <separatesWords>false</separatesWords>
        </tag>
        <standoffClass>
            <classIri>http://www.knora.org/ontology/standoff#StandoffSectionEditorTag</classIri>
        </standoffClass>
    </mappingElement>

    <mappingElement>
        <tag>
            <name>header</name>
            <class>noClass</class>
            <namespace>noNamespace</namespace>
            <separatesWords>false</separatesWords>
        </tag>
        <standoffClass>
            <classIri>http://www.knora.org/ontology/standoff#StandoffHeaderTag</classIri>
        </standoffClass>
    </mappingElement>

    <mappingElement>
        <tag>
            <name>h2</name>
            <class>noClass</class>
            <namespace>noNamespace</namespace>
            <separatesWords>true</separatesWords>
        </tag>
        <standoffClass>
            <classIri>http://www.knora.org/ontology/standoff#StandoffHeader2Tag</classIri>
        </standoffClass>
    </mappingElement>

    <mappingElement>
        <tag>
            <name>sup</name>
            <class>noClass</class>
            <namespace>noNamespace</namespace>
            <separatesWords>false</separatesWords>
        </tag>
        <standoffClass>
            <classIri>http://www.knora.org/ontology/standoff#StandoffSuperscriptTag</classIri>
        </standoffClass>
    </mappingElement>

    <mappingElement>
        <tag>
            <name>sup</name>
            <class>ckcontent</class>
            <namespace>noNamespace</namespace>
            <separatesWords>false</separatesWords>
        </tag>
        <standoffClass>
            <classIri>http://www.knora.org/ontology/knora-base#StandoffInternalReferenceTag</classIri>
            <datatype>
                <type>http://www.knora.org/ontology/knora-base#StandoffInternalReferenceTag</type>
                <attributeName>data-footnote-id</attributeName>
            </datatype>
        </standoffClass>
    </mappingElement>

    <mappingElement>
        <tag>
            <name>cite</name>
            <class>noClass</class>
            <namespace>noNamespace</namespace>
            <separatesWords>true</separatesWords>
        </tag>
        <standoffClass>
            <classIri>http://www.knora.org/ontology/standoff#StandoffCiteTag</classIri>
        </standoffClass>
    </mappingElement>
    
        <mappingElement>
            <tag>
                <name>ol</name>
                <class>noClass</class>
                <namespace>noNamespace</namespace>
                <separatesWords>true</separatesWords>
            </tag>
            <standoffClass>
                <classIri>http://www.knora.org/ontology/standoff#StandoffOrderedListTag</classIri>
            </standoffClass>
        </mappingElement>

        <mappingElement>
            <tag>
                <name>ul</name>
                <class>noClass</class>
                <namespace>noNamespace</namespace>
                <separatesWords>true</separatesWords>
            </tag>
            <standoffClass>
                <classIri>http://www.knora.org/ontology/standoff#StandoffUnorderedListTag</classIri>
            </standoffClass>
        </mappingElement>

        <mappingElement>
            <tag>
                <name>li</name>
                <class>noClass</class>
                <namespace>noNamespace</namespace>
                <separatesWords>true</separatesWords>
            </tag>
            <standoffClass>
                <classIri>http://www.knora.org/ontology/standoff#StandoffListElementTag</classIri>
            </standoffClass>
        </mappingElement>

        <mappingElement>
            <tag>
                <name>li</name>
                <class>ckcontent</class>
                <namespace>noNamespace</namespace>
                <separatesWords>false</separatesWords>
            </tag>
            <standoffClass>
                <classIri>http://www.knora.org/ontology/standoff#StandoffInternalBackReferenceTag</classIri>
                <datatype>
                    <type>http://www.knora.org/ontology/knora-base#StandoffInternalReferenceTag</type>
                    <attributeName>data-footnote-id</attributeName>
                </datatype>
            </standoffClass>
        </mappingElement>
    
</mapping>
